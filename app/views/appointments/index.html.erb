<h1> React Calendar </h1>

<h1> Appointments </h1>


<h1> Make a new appointment </h1>

 <%= simple_form_for( @appointment) do |f| %>
<%= f.input :title %>
<%= f.input :appt_time %>
<%= f.button :submit %>
<% end %>

<% @appointments.each do |appointment| %>
  <%= appointment.title %>
  <%= appointment.appt_time %>
<% end %>
